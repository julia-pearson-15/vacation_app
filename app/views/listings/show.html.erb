<h1> <%= @listing.address %> </h1>

<img src="<%= @listing.img_url %>" height="50%" width="50%">
<p>    		
    <%= @listing.price_per_night %>
    </br>
    <%= @listing.description %>
</p>       		 

<h3> Reviews: </h3>
<ul>
	<%= @reviews.each do |review| %> 
		<li>
		    <%= review.user.name %>
            </br>
			<%= @reviews.content %>
		</li>
	<% end %>
</ul>			
