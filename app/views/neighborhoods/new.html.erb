<h1>Add a Neighborhood</h1>

<%= form_for @neighborhood do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :city %>
  <%= f.text_field :city %>
  <%= f.label :state %>
  <%= f.text_field :state %>
  <%= f.label :zip %>
  <%= f.text_field :zip %>
  <%= f.label :img_url %>
  <%= f.text_field :img_url %>
<% end %>